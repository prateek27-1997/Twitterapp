<h1> Twitter</h1>
<% if user_signed_in? %>
<%= render 'count' %>
<%= render 'user_details' %>
<% end %>
<table>
  <% @tweets.each do |tweet| %>
  <tr>
      <td><%= tweet.content %></td>
      <td><%= link_to 'Show', tweet_path(tweet) %></td>
     <td><%= link_to 'Edit', edit_tweet_path(tweet) %></td>
     <td><%= link_to 'Destroy', tweet_path(tweet),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
     <td><%= link_to 'follow', relationships_path(user_id: @user_id) %></td>
     </tr>
   <% end %>
</table>












